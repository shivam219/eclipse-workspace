package swingjava;

import javax.swing.*;

public class swing1 {

	private static int i;
	private static int sum=0;
	public static void main(String[] args) {
	
	int a[]=new int [5];
	
	a[0]= 1;
	a[1]= 11;
	a[2]= 12;
	a[3]= 12;
	
	a[4]= 1;
		for(i=0;i<a.length;i++) {
			sum=sum+a[i];
		}
		System.out.println("sum is :"+sum); 
		
	}

}
