package swingjava;

import java.util.Scanner;

public class swing1 {

	public static void main(String[] args) {

		int a[] = { 20, 30, 40, 50, 60, 70, 80, 90, 100, 25, 120, 15 };

		int temp=0, i, j;
		for (i = 0; i < a.length; i++) {
			System.out.print(" " + a[i]);
		}

		for (i = 1; i < a.length; i++) {
			if (a[i]>temp) {
				temp = a[i];
				a[i] = a[i-1];
				a[i-1] = temp;
			
			}
		}
		System.out.println("after ascending");
		for(i=0;i<a.length;i++) {
			System.out.print(" "+a[i]);
		}

	}

}
