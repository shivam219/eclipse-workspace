package swingjava;

import java.util.Scanner;

public class swing1 {

	public static void main(String[] args) {

		int a[] = {20,30,40,50,60,70,80,90,100,15,25,120};
		int max=a[0];
		for (int i = 1; i <a.length; i++) {
			
			if (max>a[i]) {
				max = a[i];

			}
		}
		System.out.println("maximum :"+max);

	}

}
