package OnlinePratical;

public class Sample1 {

	public static void main(String[] args) {

		int a[]= {5,20,40,10,50,30};
		int i,n=a.length;
		System.out.print("the unsorted array is =" );
		for(i=0;i<n;i++) {
			System.out.print(a[i]+" ");
		}
		
		int temp,j;
		for(i=1;i<n;i++){
			temp=a[i];
			 j=i-1;
			 while(temp<a[j]) {
				 a[j+1]=a[j];
				 j--;
			 }
			 a[j+1]=temp;
		}
		System.out.println("the sorted array is =" );
		for(i=0;i<n;i++) {
			System.out.print(a[i]+" ");
		}
	}
}
