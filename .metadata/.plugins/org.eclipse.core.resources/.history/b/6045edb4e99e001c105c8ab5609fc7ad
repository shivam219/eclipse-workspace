package swingjava;

import java.util.Scanner;

public class swing1 {

	public static void main(String[] args) {
	
		int a[]=new int[5];
		Scanner sc=new Scanner(System.in);
		int i;
		for(i=0;i<5;i++) {
			System.out.println("enter values");
			a[i]=sc.nextInt();
		}
		
		for(i=0;i<5;i++) {
			System.out.println(a[i]);
		}
		
	}

}
