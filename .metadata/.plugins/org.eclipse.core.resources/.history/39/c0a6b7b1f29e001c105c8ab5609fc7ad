package swingjava;

import java.util.Scanner;

public class swing1 {

	public static void main(String[] args) {

		int a[] = { 20, 30, 40, 50, 60, 70, 80, 90, 100, 25, 120, 15 };

		int temp=0, i, j;
		for (i = 0; i < a.length; i++) {
			System.out.print(" " + a[i]);
		}

		for (i = 0;i < a.length; i++) {
			j=i+1;
			if (a[i]<a[j]) {
				temp = a[i];
				a[i] = a[j];
				a[j] = temp;
			
			}
		}
		System.out.println("after ascending");
		for(i=0;i<a.length;i++) {
			System.out.print(" "+a[i]);
		}

	}

}
