package swingjava;

import javax.swing.*;

public class swing1 {


	public static void main(String[] args) {
	
		int a[]= {5,4,7,8,9,6};
		int sum=0;
		int avg,i;
		for(i=0;i<a.length;i++) {
			sum=sum+a[i];
			System.out.print(" "+a[i]);
			
		}
		avg=sum/a.length;
		System.out.println(sum);
		System.out.println(avg);
		
	}

}
